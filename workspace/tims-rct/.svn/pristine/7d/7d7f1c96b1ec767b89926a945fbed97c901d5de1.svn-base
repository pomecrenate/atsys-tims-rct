html, head, body, 
header, nav, main, footer, aside, address,
article, section, area, div, pre, p,
h1, h2, h3, h4, h5, h6, 
a, span, del, ins, br, iframe,
img, em, strong, i,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend,
table, caption, summary, thead, tbody, tfoot, tr, th, td,
canvas, details, embed, audio, video,
button, input, select, textarea {
	padding: 0px;
	margin: 0px;
	font-family: 'Pretendard', sans-serif;
	white-space: normal;
	word-wrap: break-word;
	word-break: keep-all;
	vertical-align: top;
}
html, body {
	width: 100%;
	color:#747474;	
	font-size: 1rem;
	font-weight: 400;	
	letter-spacing:-0.5px;	
	line-height:1.2;	
	-webkit-font-smoothing: antialiased;
	-webkit-text-size-adjust: none;
	-webkit-tap-highlight-color: rgba(0, 0, 0, 0);
}
body{
	min-width: 20rem;
}
h1, 
h2, 
h3, 
h4, 
h5, 
h6{
	font-weight: 400;
}
ul, ol, dl{
	list-style-type: none;
}
table {
	border-collapse: collapse;
	border-spacing: 0;
}
img, fieldset {
	border: 0;
	outline: none;
}
label {
	cursor:pointer;
}
i, em, address {
	font-style: normal;
}
caption, legend {
	position: absolute;
	width: .0625rem;
	height: .0625rem;
	overflow: hidden;
	clip: rect(.0625rem, .0625rem, .0625rem, .0625rem);
	clip-path: inset(50%);
	z-index: -1;
}
img {
	max-width: 100%;
}
a {	
	color: inherit;
	cursor: pointer;
	text-decoration: none;
	-webkit-tap-highlight-color: rgba(255, 255, 255, 0);
}
a:link, 
a:visited {
	color: inherit;
	text-decoration: none;	
}
a:hover, 
a:focus, 
a:active {
	text-decoration: none;
}

button{
	font-size: 1rem;
	color: #747474;
	cursor: pointer;
	border: none;
	background: none;
}