@charset "utf-8";

body,p,h1,h2,h3,h4,h5,h6,ul,ol,li,dl,dt,dd,table,th,td,form,fieldset,legend,input,textarea,button
						{margin:0;padding:0;-webkit-text-size-adjust:none;word-break:break-all;table-layout:fixed;}
body, input,textarea,select,table,button,code,
						{font-size:12px;line-height:1.25em;font-family:'돋움', '돋움체', Dotom,Helvetica,AppleGothic,Sans-serif;}
body					{height:100%; width:100%; word-wrap:break-word; word-break:break-all; margin:0; position:absolute; background:url(/_img/stamp.gif) no-repeat; background-position:310px 125px }
.it,textarea,input
						{border:1px solid #000;}
ul,ol					{list-style:none}
table					{}
img,fieldset		{border:0}
a						{color:#007d4c;text-decoration:none;cursor:pointer}
label					{cursor:pointer}
em,address		{font-style:normal}
div					{font-family:'돋움', '돋움체', Dotom; font-size: 12px; color:#000000;}


#receipt					{}
#receipt td				{border-bottom:1px solid #0000ff; border-left:1px solid #0000ff; vertical-align:middle;}
#receipt .tdRight		{border-right:1px solid #0000ff;}
#receipt .tdTop		{border-top:1px solid #0000ff;}


.title1						{font-size:18px; font-weight:bold;text-decoration:underline; line-height:150%;}
.title2						{font-size:11px; color:#0000ff;}
.title3						{font-size:15px; color:#0000ff;}


.tdTitle						{color:#0000ff; text-align:center;font-weight:bold;vertical-align:middle;}
.tdData						{padding-left:5px;vertical-align:middle;}
.tdBottom					{color:#0000ff; text-align:center;font-size:15px;;vertical-align:middle; border-right:1px solid #0000ff;}





.textAlignLeft			{text-align:left;}
.textAlignCenter		{text-align:center;}