}
.main-visual > .inner {
  flex: 1 1 100%;
  max-width: 90rem;
  margin: 0 auto;
  padding: 9.375rem 1.25rem 0;
  box-sizing: border-box;
}
.main-visual .text {
  color: var(--navy);
  font-family: "NanumSquare Neo";
  font-size: 3.25rem;
  font-weight: 700;
  transition: font-size 0.25s;
}
.main-visual .quick-recruit {
