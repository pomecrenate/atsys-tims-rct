  margin-top: 5.625rem;
}
.main-visual .quick-recruit li {
  flex: 1 1 25%;
  background-color: rgba(160, 167, 177, 0.7);
  box-sizing: border-box;
}
.main-visual .quick-recruit li:not(:first-child) {
  border-left: solid 0.0625rem #eaeff4;
}
.main-visual .quick-recruit li > a {
  display: block;
