}
.main-visual .slide-wrap {
  position: relative;
  min-height: 13.125rem;
  margin-top: 5rem;
  overflow: hidden;
}
.main-visual .slide-wrap .btn-wrap {
  display: flex;
  -webkit-box-pack: center;
  -moz-box-justify-content: center;
  justify-content: center;
  -moz-align-items: center;
  align-items: center;
  margin: 1.25rem 0 0;
}
.main-visual .slide-wrap .btn-wrap .recruitment-pagination {
  width: auto;
  margin-right: 1rem;
  color: var(--dark);
  line-height: 1.625rem;
}
.main-visual .slide-wrap .btn-wrap .btn-slide {
  position: relative;
  width: 1.625rem;
  height: 1.625rem;
  margin: 0 0.3125rem;
  background-color: var(--w-smoke);
  border-radius: 50%;
  cursor: pointer;
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-control:before, .main-visual .slide-wrap .btn-wrap .btn-swiper-control::after {
  content: "";
  display: block;
  position: absolute;
  width: 0.125rem;
  height: 0.75rem;
  left: 50%;
  top: 50%;
  margin-top: -0.375rem;
  background-color: var(--black);
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-control::before {
  margin-left: -0.1875rem;
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-control::after {
  margin-left: 0.0625rem;
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-control.stop::before {
  width: 0;
  height: 0;
  margin-top: -0.3125rem;
  border: solid 0.3125rem transparent;
  border-left-width: 0.5rem;
  border-left-color: var(--black);
  background: none;
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-control.stop::after {
  display: none;
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-prev::after,
.main-visual .slide-wrap .btn-wrap .btn-swiper-next::after {
  content: "";
  display: block;
  position: absolute;
  width: 0.375rem;
  height: 0.375rem;
  left: 50%;
  top: 50%;
  margin: -0.25rem 0 0 -0.125rem;
  border-top: solid 0.125rem var(--black);
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-prev::after {
  border-left: solid 0.125rem var(--black);
  transform: rotate(-45deg);
}
.main-visual .slide-wrap .btn-wrap .btn-swiper-next::after {
  margin-left: -0.375rem;
  border-right: solid 0.125rem var(--black);
  transform: rotate(45deg);
}
.main-visual .slide-wrap .btn-wrap .swiper-button-disabled::after {
  opacity: 0.2;
}
