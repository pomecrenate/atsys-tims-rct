.overall_status{}
.overall_status .number_status{
	margin:0 -20px;
	display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: flex;  
}
.overall_status .number_status > div{flex:1 1 33.33%;padding:0 20px;}
.overall_status .number_status > div .x_panel{position:relative;height:calc(100% - 40px)}
.overall_status .number_status .list_title{margin-bottom:26px}
.overall_status .number_status > div:nth-child(-n+2) .x_panel > div{margin-top:4px}
.overall_status .number_status .x_panel .list_top{margin:0}
.overall_status .number_status .x_panel > div:not(.list_top){
	font-size:15px;
	display: -webkit-box;
	display: -moz-box;
	display: -ms-flexbox;
	display: flex;  
	-webkit-box-pack: justify;
	-moz-justify-content: space-between;
	-ms-flex-pack: justify;
	justify-content: space-between;
	-webkit-box-align: center;
    -moz-align-items: center;
    -ms-flex-align: center;
    align-items: center;
}
.overall_status .x_panel > div em,
.overall_status .x_panel > div span{display:block;font-style:normal;}
.overall_status .x_panel .up,
.overall_status .x_panel .down{position:relative;padding-left:20px}
.overall_status .x_panel .same{position:relative;padding-left:20px}
.overall_status .x_panel .up{color:#2974da}
.overall_status .x_panel .down{color:#ef7520}
.overall_status .x_panel .up:before,
.overall_status .x_panel .down:before{content:"";display:block;position:absolute;left:0;top:50%;margin-top:-14px;border:solid 8px transparent;border-bottom:solid 12px #2974da}
.overall_status .x_panel .down:before{margin-top:-6px;border-bottom:solid 8px transparent;border-top:solid 12px #ef7520}
.overall_status .x_panel .same:before{content:"";display:block;position:absolute;left:0;top:50%;margin-top:-1px;width:12px;height:2px;background:#666}
.overall_status .x_panel .up:after{content:"↑";display:inline-block;}
.overall_status .x_panel .down:after{content:"↓";display:inline-block;}
.overall_status .x_panel .same:after{content:"-";display:inline-block;margin-left:5px}
.overall_status .x_panel .number{color:#333;font-size:60px;font-weight:500;line-height:1}
.overall_status .float-wrap{position:relative;padding-right:400px}
.overall_status .rgt_wrap{position:absolute;right:0;top:0;width:360px;height:100%}
