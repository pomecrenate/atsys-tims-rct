   추가된 CSS
  **************************************/  
.x_panel_title {
	padding: 10px;
    margin-bottom: 5px;
    font-weight: bolder;
    font-size: 20px;
}

.x_panel_btn {
	display: flex;
    justify-content: flex-end;
    gap: 10px;
    margin-bottom: 10px;
}
  

.x_panel {
    position: relative;
    width: 100%; 
    margin-bottom: 20px;
    padding: 20px;
    display: inline-block;
    background: #fff;
    border: 1px solid #e4e8ec;
    border-radius: 15px;
    -webkit-column-break-inside: avoid;
    -moz-column-break-inside: avoid; 
    opacity: 1;
    transition: all .2s ease;
}

.search_condition {
	display: flex;
    flex-direction: column;
}

.search_condition > button {
    margin-left: auto;
}

.search_condition .condition_list > dl {
    padding: 7px 0;
}

.condition_list > dl {
    display: inline-block;
    margin-right: 20px;
    vertical-align: middle;
}

.table {
    width: 100%;
    border-collapse: collapse;
    font-size: 14px;
    table-layout: auto; 
    text-align: center;
    margin-top: 10px;
}

.table thead {
    background-color: #f5f5f5;
    font-weight: bold;
    border: 1px solid #ddd;
}

.table th, 
.table td {
    border: 1px solid #ddd;
    padding: 8px 10px;
    text-align: center;
    max-width: 300px;
    overflow: hidden;
    text-overflow: ellipsis;
    vertical-align: middle !important; /* 세로 중앙 정렬 */
}

.table tbody tr:nth-child(even) {
    background-color: #fafafa;
}

.table tbody tr:hover {
    background-color: #f0f8ff;
    cursor: pointer;
}

.description {
	margin-bottom: 5px;
}

.btn-cancel-mode {
  background-color: #e74c3c !important;
  color: white !important;
  border: 1px solid #e74c3c !important;  
}

.table input,
.table select {
	width: 120px;
  	box-sizing: border-box;
}

.table input[type="number"],
.table input[type="checkbox"] {
	width: 50px;
}

/* 페이징 */
.x_pagination {
	text-align: center;
}

.x_pagination a {
    display: inline-block;
    margin: 0 4px;
    padding: 6px 12px;
    text-decoration: none;
    color: #0096dc;
    border: 1px solid #ccc;
    border-radius: 4px;
    background-color: #fff;
    transition: all 0.2s;
}

.x_pagination a:hover {
    color: #fff !important;
    border-color: #0096dc;
    background-color: #0096dc;
}

.x_pagination a.active {
    color: #fff !important;
    border-color: #0096dc;
    background-color: #0096dc;
    font-weight: bold;
}


