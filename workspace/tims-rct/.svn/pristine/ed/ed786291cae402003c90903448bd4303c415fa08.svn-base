@CHARSET "UTF-8";

/***********************************************************Common***********************************************************/

.px-content{padding:10px}

.float_release:after{content:"";display:block;clear:both}
.lft-area {float:left;display:inline-block}
.rgt-area {float:right;display:inline-block}

.search_condition{}
.search_condition .condition_list{margin:-7px 0}
.search_condition .condition_list > dl{padding:7px 0}
.search_condition .condition_list > dl > dt{position:relative;padding-left:12px}
.search_condition .condition_list > dl > dt:after{content:"";position:absolute;left:0;top:50%;width:6px;height:6px;margin-top:-4px;border-radius:50%;background:#0096dc}

.condition_list > dl{display:inline-block;margin-right:20px;vertical-align:middle}
.condition_list > dl > dt{display:inline-block;position:relative;margin-right:12px;color:#333;line-height:30px;vertical-align:middle}
.condition_list > dl > dt label{margin:0}
.condition_list > dl > dt font{display:none !important}
.condition_list > dl > dd{display:inline-block;vertical-align:middle}
.condition_list > .n_btn{margin-left:0}

.search_condition .course_name{width:300px;}  /*org 220px;*/

.list_top{margin-bottom:15px;line-height:28px}
.list_top:after{content:"";display:block;clear:both}
.list_top .list_title,
.list_top .cont_title{margin-bottom:0;margin-right:10px;vertical-align:middle}
.list_top .detail_title{margin:0;line-height:28px}
.list_top .btn_tab{margin:0}
.list_top .check_list{display:inline-block;margin-right:20px}

.list_top .multi_select{display:inline-block;vertical-align:bottom;margin-right:5px}
.list_top .multi_select dt{float:left;margin-right:5px;line-height:30px}
.list_top .multi_select dd{float:left}

.list_top .period_wrap{display:inline-block;max-width:none;margin-right:5px;vertical-align:bottom}
.list_top .period_wrap .input-group{display:inline-block;vertical-align:bottom}

.list_title{display:inline-block;margin-bottom:15px;color:#333;font-family:'Nanum Square';font-size:18px;font-weight:700}
.list_title label,
.list_title .warning_text{font-family:'Roboto', 'Noto Sans KR', sans-serif, 'dotum', '돋움', 'arial';font-weight:400}

.dash_title{color:#333;font-family:'Nanum Square';font-size:18px;font-weight:700; text-align: center; margin-top: 12px; font-size: 22px;}

.cont_title{display:inline-block;margin-bottom:15px;color:#333;font-size:15px;font-weight:500}

.detail_title{position:relative;margin-bottom:12px;padding-left:14px;color:#333;font-size:15px;line-height:18px}
.detail_title:after{content:"";position:absolute;left:0;top:50%;width:4px;height:4px;margin-top:-2px;border-radius:50%;background:#333}

.warning_text{display:block;margin-bottom:15px;font-size:13px;color:#f00 !important;font-weight:500;line-height:18px}

.list_top .warning_text{display:inline-block;margin:0 0 0 10px;vertical-align:middle;line-height:30px}
.list_top .rgt-area .warning_text{margin-left:0;margin-right:10px}

.list_title .form_check_input,
.detail_title .form_check_input{margin:0 0 2px 10px;font-size:13px;vertical-align:middle}

.list_title .form_check_input input,
.detail_title .form_check_input input{vertical-align:middle}

.data_group{padding-top:30px}

.dp_block{display:block}
.dp_inline_block{display:inline-block}

.loading_wrap{display:none;position:fixed;left:0;top:0;width:100%;height:100%;background:rgba(0, 0, 0, 0.7);z-index:9999}
.loading_wrap .d_t{display:table;width:100%;height:100%;table-layout:fixed}
.loading_wrap .d_t .d_c{display:table-cell;width:100%;vertical-align:middle}
.loading_wrap .text{margin-top:70px;text-align:center;color:#fff}
.loader {
    margin: 0 auto;
    font-size: 25px;
    width: 25px;
    height: 25px;
    border-radius: 50%;
