.overall_status .rgt_wrap .x_panel{position:relative;height:calc(100% - 40px)}
.overall_status .rgt_wrap2 .x_panel{position:absolute;right:0;top:0;width:360px;height:100%}
.overall_status .rgt_wrap .chart_wrap{position:relative;}
