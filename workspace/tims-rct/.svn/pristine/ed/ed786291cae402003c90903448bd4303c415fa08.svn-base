/********************************************deitor*******************************************************/
#editor {border : 1px solid;width:100%;margin : 0 auto;box-sizing:border-box}	





/********************************************Text*******************************************************/
.txt-lft {text-align:left !important}
.txt-cnt {text-align:center !important}
.txt-rgt {text-align:right !important}

.text_c01{color:#0096dc !important}
.text_c02{color:#4bbf73 !important}
.text_c03{color:#ef7520 !important}

.feather{width:15px;height:15px;margin-bottom:2px;vertical-align:middle}



/********************************************Icon*******************************************************/
.icon_lock{display:inline-block;color:#0096dc;font-size:14px;vertical-align:middle}
.icon_file{display:inline-block;font-size:20px;vertical-align:middle}
.file_word{color:#558ce0 !important}
.file_pdf{color:#dc524c !important}
.file_excel{color:#52bd49 !important}
.file_ppt{color:#b7472a !important}
.file_video{color:#333 !important}
.file_img{color:#999 !important}



/********************************************Button ****************************************************/
.n_btn{display:inline-block;position:relative;color:#fff;border:solid 1px #fff;
	box-sizing:border-box-webkit-transition: all 0.2s;
	transition:all 0.2s;
}

.n_btn:disabled{color:#fff !important;border-color:#c7c7c7 !important;background:#c7c7c7 !important}

.btn_sm{height:20px;padding:0 5px;font-size:12px;line-height:18px;border-radius:3px;box-sizing:border-box}
.btn_md{height:30px;padding:0 20px;line-height:28px;border-radius:15px;box-sizing:border-box}
.btn_md2{height:21px;padding:0 11px;line-height:20px;border-radius:20px;box-sizing:border-box}
.btn_lg{height:40px;padding:0 20px;line-height:38px;border-radius:20px;box-sizing:border-box}

.btn_square{border-radius:3px !important}
.btn_organization{padding-left:30px;padding-right:10px;color:#333;border-color:#888;border-radius:3px !important;background:#fff url(../../../images/content/btn_organization.png) no-repeat 10px center}

.btn_c01{color:#fff !important;border-color:#ef7520;background-color:#ef7520}
.btn_c02{color:#fff !important;border-color:#0096dc;background-color:#0096dc}
.btn_c03{color:#fff !important;border-color:#333;background-color:#333}
.btn_c04{color:#333 !important;border-color:#888;background-color:#fff}
.btn_c05{color:#333 !important;border-color:#888;background-color:#f1f1f1}
.btn_c06{color:#fff !important;border-color:#4bbf73;background-color:#4bbf73}
.btn_c07{color:#fff !important;border-color:#8799a8;background-color:#8799a8}
.btn_c08{color:#666 !important;border-color:#ccc;background-color:#f1f1f1}
.btn_c09{color:#fff !important;border-color:#67c3b1;background-color:#67c3b1}
.btn_c10{color:#fff !important;border-color:#c2e41b;background-color:#c2e41b}
.btn_c11{color:#fff !important;border-color:#59b1e0;background-color:#59b1e0}


.btn_c01:hover,
.btn_c01.active{border-color:#cb641b;background-color:#cb641b}
.btn_c02:hover,
.btn_c02.active{border-color:#0080bb;background-color:#0080bb}
.btn_c03:hover{border-color:#333;background-color:#333}
.btn_c04:hover,
.btn_c04.active{background-color:#d9d9d9}
.btn_c05:hover,
.btn_c05.active{background-color:#cdcdcd}
.btn_c06:hover,
.btn_c06.active{border-color:#40a362;background-color:#40a362}
.btn_c07:hover,
.btn_c07.active{border-color:#73828f;background-color:#73828f}

.btn_search{margin-left:17px;padding:0 10px;border-radius:3px;background:#8799a8}
.btn_search:hover,
.btn_search.active{background:#73828f}

.btn_tab{position:relative;padding:0;margin:0 0 20px}
.btn_tab > ul{position:relative;display:inline-block;height:40px}
.btn_tab > ul:before{content:"";display:block;position:absolute;left:0;bottom:0;height:2px;width:100%;background:#0096dc;z-index:20}
.btn_tab > ul:after{content:"";display:block;clear:both}
.btn_tab > ul > li{position:relative;float:left}
.btn_tab > ul > li:first-child{z-index:10}
.btn_tab > ul > li:nth-child(2){z-index:9}
.btn_tab > ul > li:nth-child(3){z-index:8}
.btn_tab > ul > li:nth-child(4){z-index:7}
.btn_tab > ul > li:nth-child(5){z-index:6}
.btn_tab > ul > li:nth-child(6){z-index:5}
.btn_tab > ul > li:nth-child(7){z-index:4}
.btn_tab > ul > li:nth-child(8){z-index:3}
.btn_tab > ul > li:nth-child(9){z-index:2}
.btn_tab > ul > li:nth-child(10){z-index:1}
.btn_tab > ul > li a,
.btn_tab > ul > li button{display:block;position:relative;height:40px;margin-left:-8px;margin-right:24px;padding:0 6px 0 20px;color:#666;font-size:15px;line-height:40px;border:solid 1px #e4e8ec;border-radius:13px 0 0 0;border-right:none;border-bottom:none;background:#fff;box-sizing:border-box}
.btn_tab > ul > li a:after,
.btn_tab > ul > li button:after{content:"";display:block;position:absolute;right:-24px;top:-1px;width:24px;height:100%;background-image:url(../../../images/content/bg_tab_right.png);background-repeat:no-repeat;background-position:right top}
.btn_tab > ul > li:first-child a,
.btn_tab > ul > li:first-child button{margin-left:0}
.btn_tab > ul > li.active a,
.btn_tab > ul > li.active button{color:#fff !important;border-color:#0096dc;background:#0096dc}
.btn_tab > ul > li.active a:after,
.btn_tab > ul > li.active button:after{background-image:url(../../../images/content/bg_tab_on_right.png)}
.btn_tab > ul > li:hover a,
.btn_tab > ul > li:hover button{color:#0096dc}
.btn_tab > ul > li .number{position:absolute;right:10px;top:-10px;height:20px;padding:0 5px;color:#fff;text-align:center;font-size:10px;line-height:16px;border:solid 2px #fff;border-radius:10px;background:#ee843b;transition: top 0.1s ease-out}
.btn_tab > ul > li:hover > .number{top:-13px}

.btn_tab > ul:not(.tab_type02, .tab_type03) > li:last-child a,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li:last-child button{background-color:#f7f7f7 !important;}
.btn_tab > ul:not(.tab_type02, .tab_type03) > li:last-child a:after,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li:last-child button:after{background-image:url(../../../images/content/bg_tab_right02.png) !important}

.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list a,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list button{background-color:#fff !important;}
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list a:after,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list button:after{background-image:url(../../../images/content/bg_tab_right.png) !important}

.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list.active a,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list.active button{color:#fff !important;border-color:#0096dc !important;background:#0096dc !important}
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list.active a:after,
.btn_tab > ul:not(.tab_type02, .tab_type03) > li.no_list.active button:after{background-image:url(../../../images/content/bg_tab_on_right.png) !important}

.btn_tab > .tab_type02{display:block;position:static;height:35px}
.btn_tab > .tab_type02:before{height:1px;background:#888}
.btn_tab > .tab_type02 > li a,
.btn_tab > .tab_type02 > li button{height:35px;margin:0 0 0 1px;padding:0 15px;line-height:35px;border:solid 1px #d7d7d7;border-bottom:none;border-radius:3px 3px 0 0;background:#f1f1f1}
.btn_tab > .tab_type02 > li:first-child a,
.btn_tab > .tab_type02 > li:first-child button{margin-left:0}
.btn_tab > .tab_type02 > li.active{position:reltaive;z-index:100}
.btn_tab > .tab_type02 > li.active a,
.btn_tab > .tab_type02 > li.active button{color:#333 !important;border-color:#888;background:#fff}
.btn_tab > .tab_type02 > li:hover a,
.btn_tab > .tab_type02 > li:hover button{color:#333}
.btn_tab > .tab_type02 > li a:after,
.btn_tab > .tab_type02 > li button:after{display:none}

.btn_tab > .tab_type03{display:inline-block;height:30px}
.btn_tab > .tab_type03:before{display:none !important}
.btn_tab > .tab_type03 > li a,
.btn_tab > .tab_type03 > li button{height:30px;margin:0;padding:0 10px;font-size:13px;line-height:30px;border:solid 1px #aaa;border-left:none;border-radius:0;background:#d7d7d7}
.btn_tab > .tab_type03 > li:first-child a,
.btn_tab > .tab_type03 > li:first-child button{border-left:solid 1px #aaa;border-radius:3px 0 0 3px}
.btn_tab > .tab_type03 > li:last-child a,
.btn_tab > .tab_type03 > li:last-child button{border-radius:0 3px 3px 0}
.btn_tab > .tab_type03 > li.active{position:reltaive;z-index:100}
.btn_tab > .tab_type03 > li.active a,
.btn_tab > .tab_type03 > li.active button{color:#333 !important;border-color:#aaa;background:#fff}
.btn_tab > .tab_type03 > li:hover a,
.btn_tab > .tab_type03 > li:hover button{color:#333}
.btn_tab > .tab_type03 > li a:after,
.btn_tab > .tab_type03 > li button:after{display:none}

