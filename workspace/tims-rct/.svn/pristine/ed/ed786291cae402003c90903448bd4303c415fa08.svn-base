form:after{display:none !important}

input[type="radio"],
input[type="checkbox"]{margin:0;padding:0}
label{margin:0;color:#666;font-weight:400}

.hide_label{position:absolute;right:-9999px;top:-9999px;width:0;height:0;margin:0;padding:0;font-size:0;line-height:0;opactiy:0}

.form-control{color:#666;font-size:13px;font-weight:400}
select.form-control{padding-right:25px;-webkit-appearance:none;-moz-appearance:none;appearance:none;background:url(../../../images/content/select_arrow.png) no-repeat right 8px center}
select.form-control::-ms-expand {display:none;}	

.form_inline_block{display:inline-block;width:auto}

.form_check_input{display:inline-block;position:relative}
.form_check_input input[type="checkbox"],
.form_check_input input[type="radio"]{position:absolute;left:2%;top:50%;width:0;height:0;border:0;background:none;opacity:0;vertical-align:middle}
.form_check_input label{position:relative;cursor:pointer;padding-left:19px;vertical-align:middle}
.form_check_input label:before{content:"";display:inline-block;position:absolute;left:0;top:50%;width:14px;height:14px;margin-top:-7px;border:solid 1px #d8d8d8;border-radius:50%;background-color:#fff;vertical-align:middle}
.form_check_input input[type="checkbox"]:checked + label:before,
.form_check_input input[type="radio"]:checked + label:before{border-color:#0096dc;background-color:#0096dc}
.form_check_input input[type="radio"]:checked + label:after{content:"";display:block;position:absolute;left:4px;top:50%;width:6px;height:6px;margin-top:-3px;border-radius:50%;background:#fff}
.form_check_input input[type="checkbox"] + label:before{border-radius:3px;}
.form_check_input input[type="checkbox"]:checked + label:before{background-image:url(../../../images/content/input_check.png);background-repeat:no-repeat;background-position:center center}
.form_check_input input[type="checkbox"]:disabled + label:before,
.form_check_input input[type="radio"]:disabled + label:before{border-color:#c7c7c7;background-color:#c7c7c7}
.form_check_input input[type="radio"]:disabled + label:before{background:#fff;}
.form_check_input input[type="radio"]:checked:disabled + label:after{background-color:#c7c7c7}


